<!DOCTYPE html>
<html>
<head>
  <title>Book List</title>
</head>
<body>
  <h1>Book List</h1>
  <table border="solid black 1px">
    <thead>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Year</th>
        <th>Genre</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% books.forEach(book => { %>
        <tr>
          <td><%= book.id %></td>
          <td><%= book.title %></td>
          <td><%= book.year %></td> 
          <td><%= book.genre %></td>
          <td>
            <a href="/edit/<%= book.id %>">Edit</a> | 
            <a href="/delete/<%= book.id %>">Delete</a>
          </td>
        </tr>
      <% }); %>
    </tbody>
  </table>
  <a href="/add">Add Book</a>
</body>
</html>